{
    "name": "conway_github",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "gh.png"
    }
}
